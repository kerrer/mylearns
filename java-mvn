mvn  archetype:generate -DgroupId=org.holbreich -DartifactId=filecopy -DarchetypeGroupId=org.apache.camel.archetypes -DarchetypeArtifactId=camel-archetype-java -Dversion=1.0.0-SNAPSHOT


mvn clean compile exec:java -Dexec.mainClass="com.max.shiro.Tutorial"

mvn clean jetty:run -s ~/.sdkman/candidates/maven/current/conf/settings.xml -Dmaven.test.skip=true

mvn  jetty:run  -Dmaven.test.skip=true -U

java -jar target/dropwizard-example-1.0.1-SNAPSHOT.jar db migrate example.yml
